{
	"expo": {
		"name": "Offentlige tilfluktsrom i Norge",
		"slug": "Offentlige_tilfluktsrom_i_Norge",
		"privacy": "public",
		"sdkVersion": "32.0.0",
		"platforms": [ "ios", "android" ],
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./app/assets/icon.png",
		"splash": {
			"image": "./app/assets/icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#212121"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [ "**/*" ],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.semanta.sheltersNorway"
		},
		"android": {
			"package": "com.semanta.sheltersNorway",
			"permissions": [
				"CAMERA",
				"RECORD_AUDIO",
				"READ_PHONE_STATE",
				"READ_CONTACTS",
				"ACCESS_FINE_LOCATION",
				"ACCESS_COARSE_LOCATION",
				"com.google.android.c2dm.permission.RECEIVE",
				"com.android.launcher.permission.INSTALL_SHORTCUT",
				"com.htc.launcher.permission.UPDATE_SHORTCUT"
			]
		}
	}
}
